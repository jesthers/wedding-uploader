# Google OAuth Credentials (Google Cloud Console > APIs & Services > Credentials)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# OAuth redirect URL registered in the same OAuth client (localhost 예시)
GOOGLE_REDIRECT_URI=http://localhost:4000/auth/google/callback

# Google Drive folder ID where guest folders/files will be created
# Drive URL 예시: https://drive.google.com/drive/folders/ABCDEFG...
# 위 URL의 마지막 부분(ABCDEFG...)이 폴더 ID입니다.
GOOGLE_DRIVE_PARENT_FOLDER_ID=your-parent-folder-id

# Express server port
PORT=4000
